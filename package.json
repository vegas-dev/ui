{
	"name": "@vgapp/ui",
	"version": "1.0.0",
	"description": "A hodgepodge of plugins and web applications that can work together and complement each other",
	"scripts": {
		"clean": "rimraf dist/{css/*,js/*}",
		"scss": "node-sass --output-style compressed -o dist/css sources/scss/app.scss",
		"babel": "mkdirp dist/js -p && babel sources/js/app.js -o dist/js/vgapp.ui.js && babel --presets minify sources/js/app.js -o dist/js/vgapp.ui.min.js",
		"build:css": "run-s scss",
		"build:js": "run-s babel",
		"build": "run-s build:*",
		"watch:css": "onchange \"sources/scss\" -- run-s build:css",
		"watch:js": "onchange \"sources/js\" -- run-s build:js",
		"watch": "run-p watch:*"
	},
	"author": {
		"name": "Vegas Studio",
		"url": "https://vegas-dev.com",
		"email": "vegas.dev.studio@gmail.com"
	},
	"keywords": [
		"javascript",
		"navigation",
		"sidebar"
	],
	"license": "SEE LICENSE IN LICENSE.md",
	"homepage": "https://vegas-dev.com/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vegas-dev/ui.git"
	},
	"bugs": {
		"url": "https://github.com/vegas-dev/vegas-sidebar/issues"
	},
	"devDependencies": {
		"@babel/cli": "^7.11.6",
		"@babel/core": "^7.11.6",
		"@babel/preset-env": "^7.11.5",
		"babel-preset-minify": "^0.5.1",
		"mkdirp": "^0.5.1",
		"node-sass": "^4.14.1",
		"npm-run-all": "^2.1.1",
		"onchange": "^2.4.0",
		"rimraf": "^2.5.4"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not op_mini all"
	],
	"dependencies": {}
}
